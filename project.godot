; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="2018game"
run/main_scene="res://scenes/Menu.tscn"
run/low_processor_mode=true
config/icon="res://icon.png"

[audio]

channel_disable_threshold_db=-100.0

[autoload]

global="*res://global.gd"

[debug]

settings/stdout/print_fps=true
shapes/collision/shape_color=Color( 0.317187, 0.7, 0, 0.22098 )
shapes/collision/contact_color=Color( 1, 0.2, 0.1, 0.171294 )

[display]

window/size/width=480
window/size/height=270
window/size/fullscreen=true
window/size/test_width=1600
window/size/test_height=900
window/vsync/use_vsync=false
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[editor_plugins]

enabled=PoolStringArray( "gamejolt_api_v2" )

[importer_defaults]

